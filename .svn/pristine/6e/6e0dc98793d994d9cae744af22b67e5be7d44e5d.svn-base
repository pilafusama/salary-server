package com.tenpay.wxwork.salary.model;

public class CardBin {
    public static final int DEBIT_CARD = 1; // 借记卡
    public static final int CREDIT_CARD = 2; // 信用卡

    private int cardType;

    private String bankSname;

    private String cardNumber;

    @Override
    public String toString() {
        return String.format("bank sname: %s, card type: %d", bankSname, cardType);
    }

    public int getCardType()
    {
        return cardType;
    }

    public void setCardType(int cardType)
    {
        this.cardType = cardType;
    }

    public String getBankSname()
    {
        return bankSname;
    }

    public void setBankSname(String bankSname)
    {
        this.bankSname = bankSname;
    }

    public String getCardNumber()
    {
        return cardNumber;
    }

    public void setCardNumber(String cardNumber)
    {
        this.cardNumber = cardNumber;
    }
}