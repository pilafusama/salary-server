package com.tenpay.fingate.http;


/**
 * http请求的响应处理
 * 
 * @author robenzhang
 */
public class HttpClient4 {
	// 通过get请求调用http连接，不调用连接池
	public static HttpClient4ResponseResult doGet(String url) throws Exception {
		HttpClient4Util httpClient4Util = new HttpClient4Util();
		return httpClient4Util.doHttpGet(url);
	}

}